<%- include('../partials/header') -%>
<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% movies.forEach(movie => { %>
    <tr>
      <td><%= movie.title %></td>
      <td><%= movie.releaseYear %></td>
      <td><a href="/movies/<%= movie._id %>">DETAILS</a></td>
    </tr>
    <% }); %>
  </tbody>
</table>
<%- include('../partials/footer') -%>
